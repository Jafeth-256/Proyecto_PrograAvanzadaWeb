<script>
    document.addEventListener('DOMContentLoaded', function() {
        // Animaciones para las info cards
        const infoCards = document.querySelectorAll('.info-card');
        infoCards.forEach((card, index) => {
            card.style.opacity = '0';
            card.style.transform = 'translateY(20px)';

            setTimeout(() => {
                card.style.transition = 'all 0.5s ease';
                card.style.opacity = '1';
                card.style.transform = 'translateY(0)';
            }, index * 150);
        });

        // Animación para el contenido principal
        const mainCard = document.querySelector('.main-content .table-card');
        const sidebarCard = document.querySelector('.sidebar-content .table-card');

        if (mainCard) {
            mainCard.style.opacity = '0';
            mainCard.style.transform = 'translateX(-20px)';
            setTimeout(() => {
                mainCard.style.transition = 'all 0.6s ease';
                mainCard.style.opacity = '1';
                mainCard.style.transform = 'translateX(0)';
            }, 200);
        }

        if (sidebarCard) {
            sidebarCard.style.opacity = '0';
            sidebarCard.style.transform = 'translateX(20px)';
            setTimeout(() => {
                sidebarCard.style.transition = 'all 0.6s ease';
                sidebarCard.style.opacity = '1';
                sidebarCard.style.transform = 'translateX(0)';
            }, 400);
        }

        // Animación especial para el botón de reservar
        const reservarBtn = document.querySelector('.btn-primary.btn-large');
        if (reservarBtn) {
            reservarBtn.addEventListener('click', function(e) {
                this.style.transform = 'scale(0.95)';
                setTimeout(() => {
                    this.style.transform = 'translateY(-2px) scale(1)';
                }, 150);
            });
        }

        // Animación para la barra de capacidad
        const capacityFill = document.querySelector('.capacity-fill');
        if (capacityFill) {
            const targetWidth = capacityFill.style.width;
            capacityFill.style.width = '0%';
            setTimeout(() => {
                capacityFill.style.transition = 'width 1s ease-out';
                capacityFill.style.width = targetWidth;
            }, 800);
        }

        // Efecto hover mejorado para las highlight items
        const highlightItems = document.querySelectorAll('.highlight-item');
        highlightItems.forEach(item => {
            item.addEventListener('mouseenter', function() {
                this.style.transform = 'translateX(5px)';
                this.style.boxShadow = '0 4px 15px rgba(16, 185, 129, 0.1)';
            });

            item.addEventListener('mouseleave', function() {
                this.style.transform = 'translateX(0)';
                this.style.boxShadow = 'none';
            });
        });
    });
</script>